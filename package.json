    "@nuxt/eslint": "^1.12.1",
    "@nuxt/fonts": "^0.12.1",
    "@nuxt/icon": "^2.1.1",
    "@nuxt/image": "^2.0.0",
    "@nuxt/ui": "^3.3.7",
    "@nuxtjs/robots": "^5.6.7",
    "@nuxtjs/sitemap": "^7.5.0",
    "@vite-pwa/nuxt": "^1.1.0",
    "better-sqlite3": "^12.5.0",
    "nuxt": "^3.20.2",
    "nuxt-schema-org": "^5.0.10",
